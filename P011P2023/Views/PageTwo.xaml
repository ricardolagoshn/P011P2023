<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="P011P2023.Views.PageTwo">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding nombres}"
               
                HorizontalOptions="CenterAndExpand" x:Name="lbnombre" />

            <Label Text="{Binding apellidos}"
                 
                HorizontalOptions="CenterAndExpand" x:Name="lbapellidos" />

            <Label Text="{Binding telefono}"
               
                HorizontalOptions="CenterAndExpand" x:Name="lbtelefono" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>